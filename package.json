{
  "name": "pass-up",
  "version": "0.0.10",
  "description": "Glue DOM and Custom Elements together.",
  "keywords": [
    "webcomponents",
    "webcomponent",
    "customelements",
    "customelement",
    "bind",
    "dom-bind"
  ],
  "main": "pass-up.js",
  "module": "pass-up.js",
  "exports": {
    ".": "./p-u.js",
    "./p-u.js": "./p-u.js"
  },
  "files": [
    "p*.js",
    "p*.ts",
    "types.d.ts",
    "custom-elements.json"
  ],
  "types": "./types.d.ts",
  "scripts": {
    "test": "node test/test.js",
    "package-check": "package-check",
    "serve": "node node_modules/xtal-test/serve.js",
    "doc": "custom-elements-manifest analyze"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/pass-up.git"
  },
  "dependencies": {
    "on-to-me": "0.0.35",
    "trans-render": "0.0.401"
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "xtal-test": "0.0.51",
    "@custom-elements-manifest/analyzer": "0.5.3"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/pass-up/issues"
  },
  "homepage": "https://github.com/bahrus/pass-up#readme",
  "customElements": "custom-elements.json"
}
